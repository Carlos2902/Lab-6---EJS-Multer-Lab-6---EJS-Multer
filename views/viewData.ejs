<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>View Data</title>
    <link rel="stylesheet" href="/public/css/main">
</head>
<body>
    <table border="1">
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Occupation</th>
            <th>Company</th>
        </tr>
        <!-- <% if (data.visible){%> -->
            <% data.forEach(user=>{%>

                <tr>
                    <td><%= user.name %></td>
                    <td><%= user.age %></td>
                    <td><%= user.occupation %></td>
                    <td><%= user.company %></td>
                </tr>
            <%}) %>
        <!-- <%}else{%> -->
            <!-- <tr>
                <td colspan="4">User: '<%= data.name%>' has hidden their information</td>
            </tr> -->
        <!-- <%} %> -->
    </table>


    <%- include('partials/footer', {page: 'viewData'}) %>

</body>
</html>